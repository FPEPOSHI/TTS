{
    "model_name": "TTS-mel_spec-r=2",
    "model_description": "Predict only mel spectrogram with r=2",

    "audio":{
        "audio_processor": "audio",
        "num_mels": 80,
        "num_freq": 1025,
        "sample_rate": 22000,
        "frame_length_ms": 50,
        "frame_shift_ms": 12.5,
        "preemphasis": 0.0,
        "min_level_db": -100,
        "ref_level_db": 20,
        "power": 1.5,
        "griffin_lim_iters": 60 
    },

    "embedding_size": 256,
    "text_cleaner": "english_cleaners",
    "epochs": 1000,
    "lr": 0.0015,
    "warmup_steps": 4000,
    "batch_size":32,
    "eval_batch_size":32,
    "r": 2,
    "wd": 0.000001,
    "checkpoint": true,
    "save_step": 25000,
    "print_step": 10,

    "run_eval": true,
    "data_path": "../../Data/LJSpeech-1.1/tts_cache",
    "meta_file_train": "tts_meta_data.csv",
    "meta_file_val": "tts_meta_data_val.csv",
    "data_loader": "TTSDatasetMemory",
    "dataset": "tts_cache",
    "min_seq_len": 0,
    "output_path": "../keep/",
    "num_loader_workers": 8
}